<table class="table table-striped table-hover">
    <caption>Categories List</caption>
    <thead>
    <tr>
        <th>#</th>
        <th>Название</th>
        <th>Затраты</th>
        <th>Дата</th>
    </tr>
    </thead>
    <tbody>
    {{#each categories as |item index|}}
        <tr>
            <td>{{index}}</td>
            <td>{{item.name}}</td>
            <td>{{item.cost}}</td>
            <td>{{item.date}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>